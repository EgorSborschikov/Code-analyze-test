<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net8.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
	<OutputType>Library</OutputType>
    <Nullable>enable</Nullable>
	<PackageId>Todo.Core</PackageId>
	<Version>1.0.0</Version>
	<Authors>EgorSborschikov</Authors>
	<Description>Библиотека для управления списком дел.</Description>
	<PackageProjectUrl>https://github.com/EgorSborschikov/Code-analyze-test</PackageProjectUrl>
  	<RepositoryUrl>https://github.com/EgorSborschikov/Code-analyze-test</RepositoryUrl>
	<GeneratePackageOnBuild>true</GeneratePackageOnBuild>
	<GenerateDocumentationFile>true</GenerateDocumentationFile>
  </PropertyGroup>

	<ItemGroup>
		<PackageReference Include="StyleCop.Analyzers" Version="1.1.118" PrivateAssets="all"/>
		<PackageReference Include="Microsoft.CodeAnalysis.NetAnalyzers" Version ="9.0.0" PrivateAssets="all"/>
	</ItemGroup>
</Project>
